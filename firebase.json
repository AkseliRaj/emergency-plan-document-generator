{
  "hosting": {
    "public": "emergency-plan-document-generator/dist",
    "predeploy": [
      "npm --prefix emergency-plan-document-generator run build"
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
